<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="250.5dp"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@drawable/bg_dialog"
    >
    <LinearLayout
        android:layout_width="250.5dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
        <TextView
            android:id="@+id/tvTitle"
            android:layout_marginTop="20dp"
            android:layout_width="250.5dp"
            android:layout_height="wrap_content"
            android:text="XXXXX"
            android:textColor="@color/black"
            android:textSize="17dp"
            android:textStyle="bold"
            android:gravity="center_horizontal"
            />
        <TextView
            android:layout_width="250.5dp"
            android:layout_height="0.5dp"
            android:background="#828282"
            android:layout_marginTop="30dp"
            />
        <RelativeLayout
            android:layout_width="250.5dp"
            android:layout_height="wrap_content"
            >
            <TextView
                android:id="@+id/f_quchecbutton_btn_queding"
                android:layout_width="125dp"
                android:layout_height="40dp"
                android:text="确定"
                android:textSize="16dp"
                android:gravity="center"
                android:textColor="#0888ff"
                />
            <TextView
                android:id="@+id/and"
                android:layout_width="0.5dp"
                android:layout_height="40dp"
                android:background="#828282"
                android:layout_toRightOf="@id/f_quchecbutton_btn_queding"
                android:textSize="16sp"
                android:gravity="center"
                />
            <TextView
                android:id="@+id/f_quchecbutton_btn_quxiao"
                android:layout_toRightOf="@id/and"
                android:layout_width="125dp"
                android:layout_height="40dp"
                android:text="取消"
                android:textSize="16dp"
                android:gravity="center"
                android:textColor="#0888ff"
                />
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>