<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="81dp">
    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/teacherlistImg"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="7dp"
        android:layout_marginLeft="12dp"
        android:layout_marginBottom="14dp" />
    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="wrap_content"
        android:layout_height="52dp"
        android:layout_toRightOf="@id/teacherlistImg"
        android:layout_marginTop="29dp"
        android:layout_marginLeft="10dp">
        <ImageView
            android:id="@+id/teacherlistIcon1"
            android:layout_marginTop="4dp"
            android:layout_width="8.5dp"
            android:layout_height="8.5dp"/>
        <TextView
            android:id="@+id/teacherlistCountry"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:includeFontPadding="false"
            android:layout_alignParentRight="true"
            android:layout_marginTop="5dp"
            android:layout_marginRight="19dp"
            android:textColor="#626362"
            android:textSize="12dp"
            android:text="bbbbbbb">
        </TextView>
        <ImageView
            android:id="@+id/teacherlistIcon2"
            android:layout_marginTop="6dp"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginRight="2.5dp"
            android:src="@mipmap/coordination"
            android:layout_toLeftOf="@id/teacherlistCountry"/>
        <TextView
            android:id="@+id/teacherlistName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:includeFontPadding="false"
            android:layout_marginLeft="2.5dp"
            android:layout_toRightOf="@id/teacherlistIcon1"
            android:textSize="17dp"
            android:textColor="#111111"
            android:maxLines="1"
            android:ellipsize="end"
            android:layout_toLeftOf="@id/teacherlistIcon2"
            android:text="aaaaaaaa"/>

    </RelativeLayout>
    <View
        android:id="@+id/viewLine"
        android:layout_below="@id/relativeLayout"
        android:layout_marginLeft="82dp"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="#f5f5f5">
    </View>
</RelativeLayout>